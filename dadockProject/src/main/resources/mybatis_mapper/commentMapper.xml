<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybaits.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="com.acorn.dadockProject.mapper.CommentMapper">
	<select id="selectByIsbnAll" resultType="Comment" parameterType="String">
		select * from comment where isbn=#{isbn}
	</select>
	
	<select id="selectByUserAll" resultType="Comment" parameterType="String">
		select * from comment where user_id=#{user_id}
	</select>
	
	
</mapper>